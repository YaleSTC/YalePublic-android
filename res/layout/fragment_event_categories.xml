<!-- Created by Stan Swidwinski
   General layout for displaying buttons for categories of events within the events functionality.
-->
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingLeft="0dp"
    android:paddingRight="0dp"
    android:paddingTop="0dp"
    android:paddingBottom="0dp"
    tools:context="edu.yale.yalepublic.Events.EventCategories$PlaceholderFragment">

    <ListView
        android:id="@+id/listview_event_categories"
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:divider="@drawable/separator_events"
        android:dividerHeight="2dp"/>
</RelativeLayout>
